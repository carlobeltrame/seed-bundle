{
    "name": "evotodi/seed-bundle",
    "description": "Database seeder for symfony 4",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "evotodi",
            "email": "evotodi@gmail.com"
        },
        {
            "name": "Soyuka",
            "email": "soyuka@gmail.com"
        }
    ],
    "require": {
        "php": "7.*",
        "symfony/framework-bundle": "4.4.*|5.0.*|5.1.*",
        "symfony/console": "4.4.*|5.0.*|5.1.*",
        "symfony/finder": "4.4.*|5.0.*|5.1.*",
        "doctrine/doctrine-bundle": "^1.0|^2.0",
        "doctrine/orm": "^2.7",
        "symfony/yaml": "4.4.*|5.0.*|5.1.*"
    },
    "require-dev": {
        "symfony/config": "4.4.*|5.0.*|5.1.*",
        "symfony/dependency-injection": "4.4.*|5.0.*|5.1.*",
        "symfony/phpunit-bridge": "4.4.*|5.0.*|5.1.*"

    },
    "autoload": {
        "psr-4": {
            "Evotodi\\SeedBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Evotodi\\SeedBundle\\Tests\\": "tests/"
        }
    }
}
